import strutils,sequtils

proc dir_criteria(inp:string): bool =
  if inp.len() != 0:
    return true
  else:
    return false

proc PruneArgs(commands:string):seq[string]=
  return commands.split(" ").filter(dir_criteria)

proc PruneArgs(commands:seq[string]):seq[string]=
  return commands.filter(dir_criteria)

proc PruneArgs(commands:varargs[string]):seq[string]=
  return commands.filter(dir_criteria)
